#! /usr/bin/vvp
:ivl_version "12.0 (stable)" "(v12_0-dirty)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/ivl/system.vpi";
:vpi_module "/usr/lib/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/ivl/va_math.vpi";
S_0x5aa5cb09bb00 .scope module, "testbench" "testbench" 2 20;
 .timescale 0 0;
v0x5aa5cb0ac9d0_0 .var "clk", 0 0;
S_0x5aa5cb09bc90 .scope module, "uut" "read_instructions" 2 24, 2 1 0, S_0x5aa5cb09bb00;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
v0x5aa5cb061470_0 .net "clk", 0 0, v0x5aa5cb0ac9d0_0;  1 drivers
v0x5aa5cb0ac810 .array "instructions", 4 0, 31 0;
v0x5aa5cb0ac8d0_0 .var/i "scanned", 31 0;
E_0x5aa5cb061e50 .event posedge, v0x5aa5cb061470_0;
    .scope S_0x5aa5cb09bc90;
T_0 ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5aa5cb0ac8d0_0, 0, 32;
    %vpi_call 2 7 "$readmemb", "/home/chiranjeet/Text File.txt", v0x5aa5cb0ac810 {0 0 0};
    %end;
    .thread T_0;
    .scope S_0x5aa5cb09bc90;
T_1 ;
    %wait E_0x5aa5cb061e50;
    %load/vec4 v0x5aa5cb0ac8d0_0;
    %cmpi/s 5, 0, 32;
    %jmp/0xz  T_1.0, 5;
    %vpi_call 2 12 "$display", "Instruction %0d: %b", v0x5aa5cb0ac8d0_0, &A<v0x5aa5cb0ac810, v0x5aa5cb0ac8d0_0 > {0 0 0};
    %load/vec4 v0x5aa5cb0ac8d0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x5aa5cb0ac8d0_0, 0, 32;
    %jmp T_1.1;
T_1.0 ;
    %vpi_call 2 15 "$display", "End of Instructions." {0 0 0};
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x5aa5cb09bb00;
T_2 ;
    %delay 5, 0;
    %load/vec4 v0x5aa5cb0ac9d0_0;
    %inv;
    %store/vec4 v0x5aa5cb0ac9d0_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x5aa5cb09bb00;
T_3 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5aa5cb0ac9d0_0, 0, 1;
    %delay 200, 0;
    %vpi_call 2 32 "$stop" {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "fetch.sv";
